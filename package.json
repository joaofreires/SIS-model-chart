{
  "name": "sis-model",
  "version": "1.0.0",
  "description": "SIS model chart",
  "main": "./src/js/main",
  "scripts": {
    "serve": "browser-sync start --server \"dist\" --files \"dist\"",
    "test": "test",
    "watch": "run-p serve watch:*",
    "build": "run-p build:*",
    "build:js": "webpack --mode=production",
    "watch:ts": "onchange \"src/ts\" -- webpack --mode=development",
    "build:html": "posthtml -c posthtml.json",
    "watch:html": "onchange \"src/views\" -- npm run build:html",
    "css:scss": "node-sass --include-path ./node_modules --output-style compressed -o dist src/scss",
    "css:autoprefixer": "postcss -u autoprefixer -r dist/*.css",
    "build:css": "npm run css:scss && npm run css:autoprefixer",
    "watch:css": "onchange \"src/scss\" -- npm run build:css"
  },
  "author": "Joao Freires",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@babel/preset-env": "^7.12.17",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "autoprefixer": "^10.2.4",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.26.14",
    "css-loader": "^5.0.2",
    "eslint": "^7.20.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "htmlnano": "^0.2.8",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "postcss": "^8.2.6",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^4.2.0",
    "posthtml": "^0.15.1",
    "posthtml-cli": "^0.9.1",
    "posthtml-modules": "^0.7.3",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.17",
    "typescript": "^4.1.5",
    "webpack": "^5.23.0",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "@types/lodash": "^4.14.168",
    "bootstrap": "^4.6.0",
    "chart.js": "^3.0.0-beta.10",
    "jquery": "^3.5.1",
    "lodash": "^4.17.20",
    "popper.js": "^1.16.1"
  }
}
